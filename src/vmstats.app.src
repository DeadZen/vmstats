{application, vmstats, [
  {description, "Tiny application to gather VM statistics for StatsD client"},
  {vsn, "0.3.0"},
  {registered, [
      vmstats_sup,
      vmstats_server
  ]},
  {applications, [
    kernel,
    stdlib,
    statsderl
  ]},
  {mod, {vmstats, []}},
  {env, [
    {prefix_key, ""},
    {base_key, "vmstats"}
  ]},
  {applications, [statsderl]},
  {modules, [
      vmstats,
      vmstats_sup,
      vmstats_server
  ]}
]}.
